{
	"name": "a3stacksvelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:start": "docker compose up",
		"db:migrate": "atlas migrate apply --dir \"file://migrations\" --url $DATABASE_URL",
		"db:migrate:status": "atlas migrate status --dir \"file://migrations\" --url $DATABASE_URL",
		"db:migrate:diff": "atlas migrate diff --dir \"file://migrations\" --to \"env://local\" --dev-url \"docker://postgres/15\"",
		"db:seed": "bun run scripts/seed-todos.ts",
		"db:seed:todos": "bun run scripts/seed-todos.ts",
		"stripe:seed": "bun run scripts/stripe-seed.ts",
		"gentypes": "kysely-codegen --include-pattern=\"public.*\" --dialect=postgres --out-file=src/lib/server/db/db.types.ts",
		"db:setup": "bun run db:migrate && bun run gentypes"
	},
	"devDependencies": {
		"@dnd-kit-svelte/core": "^0.0.8",
		"@dnd-kit-svelte/modifiers": "^0.0.8",
		"@dnd-kit-svelte/sortable": "^0.0.8",
		"@dnd-kit-svelte/utilities": "^0.0.8",
		"@eslint/compat": "^1.3.1",
		"@eslint/js": "^9.32.0",
		"@internationalized/date": "^3.8.2",
		"@lucide/svelte": "^0.515.0",
		"@sveltejs/kit": "^2.27.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"@tabler/icons-svelte": "^3.34.1",
		"@tailwindcss/vite": "^4.1.11",
		"@tanstack/table-core": "^8.21.3",
		"@types/bun": "^1.2.19",
		"@types/d3-scale": "^4.0.9",
		"@types/d3-shape": "^3.1.7",
		"bits-ui": "^2.9.1",
		"clsx": "^2.1.1",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.2.0",
		"embla-carousel-svelte": "^8.6.0",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"formsnap": "^2.0.1",
		"globals": "^16.3.0",
		"kysely-codegen": "^0.18.5",
		"kysely-pglite": "^0.6.1",
		"layerchart": "2.0.0-next.27",
		"mode-watcher": "^1.1.0",
		"paneforge": "^1.0.2",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.37.2",
		"svelte-adapter-bun": "^0.5.2",
		"svelte-check": "^4.3.0",
		"svelte-sonner": "^1.0.5",
		"sveltekit-superforms": "^2.27.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.11",
		"tw-animate-css": "^1.3.6",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.38.0",
		"valibot": "^1.1.0",
		"vaul-svelte": "^1.0.0-next.7",
		"vite": "npm:rolldown-vite@latest"
	},
	"dependencies": {
		"@better-auth/stripe": "^1.3.4",
		"@electric-sql/pglite": "^0.3.7",
		"@elysiajs/eden": "^1.3.2",
		"@types/pg": "^8.15.5",
		"better-auth": "^1.3.4",
		"elysia": "^1.3.8",
		"kysely": "^0.28.5",
		"pg": "^8.16.3",
		"stripe": "^18.0.0"
	}
}
